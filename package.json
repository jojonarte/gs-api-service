{
  "name": "api-service",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && tsc --build",
    "prestart": "yarn build",
    "start": "ts-node -p src/server.ts",
    "serve": "mkdir dist/public && cd dist && node ./server.js",
    "test": "ENV_FILE=./.env jest --setupFiles dotenv/config",
    "lint": "npx eslint . --ext .ts,.tsx",
    "ts-check": "tsc --noEmit --skipLibCheck",
    "precommit": "yarn lint; yarn ts-check; pretty-quick --staged"
  },
  "dependencies": {
    "@types/express": "^4.17.12",
    "@types/koa-bodyparser": "^4.3.1",
    "@types/koa-router": "^7.4.2",
    "@types/node": "^16.0.0",
    "@types/pino": "^6.3.8",
    "@types/ramda": "^0.27.42",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "knex": "^0.95.6",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-router": "^10.0.0",
    "objection": "^1.6.9",
    "objection-db-errors": "^1.1.1",
    "pg": "^8.6.0",
    "pino": "^6.11.3",
    "pino-pretty": "^5.1.0",
    "ramda": "^0.27.1",
    "typescript": "^4.3.5"
  },
  "_moduleAliases": {
    "src": "dist"
  },
  "devDependencies": {
    "eslint": "^7.30.0",
    "jest": "^27.0.6",
    "module-alias": "^2.2.2",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "ts-check": "^0.2.4",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0"
  }
}
